{{!--outlet--}}
<nav>
    <div class="nav-wrapper">
        <a href="#" class="brand-logo">Registro de Usuario</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
            <li>
                {{#link-to 'login'}}
                    Iniciar Sesión
                {{/link-to}}

            </li>
        </ul>
    </div>
</nav>

<div class="row">
    <form class="col s12">
        <div class="row">
            <div class="input-field col s6">
                {{input id="first_name" type="text" class="validate" value=nombre}}
                <label for="first_name">Nombre</label>
            </div>
            <div class="input-field col s6">
                {{input id="last_name" type="text" class="validate" value=apellido}}
                <label for="last_name">Apellido</label>
            </div>
        </div>

        <div class="row">
            <div class="input-field col s12">
                {{input id="email" type="email" class="validate" value=email}}
                <label for="email">Correo electrónico</label>
            </div>
        </div>
     
        <div class="row">
            <div class="input-field col s12">
                {{input id="password" type="password" class="validate" value=password}}
                <label for="password">Contraseña</label>
            </div>
        </div>

        <!--div class="row">
            <div class="input-field col s12">
                {{input disabled="No" value="No disponible" id="disabled" type="text" class="validate" value=cveAdmin}}
                <label for="disabled">Clave de Administrador</label>
            </div>
        </div-->

        <div class="row">
            <button {{action 'registroUsuario'}} class="btn waves-effect waves-light" type="submit" name="action">Regístrate   
            </button>
        </div>
     
    </form>
</div>